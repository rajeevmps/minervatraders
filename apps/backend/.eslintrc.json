{
    "root": true,
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": [
            "warn",
            {
                "argsIgnorePattern": "^next$"
            }
        ]
    }
}